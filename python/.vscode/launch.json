{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "format_all",
            "type": "python",
            "pythonPath":"${config:python.pythonPath}",
            "request": "launch",
            "stopOnEntry": false,
            "console": "integratedTerminal",
            "program": "${workspaceRoot}/format_all.py",
            "cwd": "${workspaceRoot}",
            "debugOptions": [
                "WaitOnAbnormalExit",
                "WaitOnNormalExit",
                "RedirectOutput"
            ]
        }
    ]
}
